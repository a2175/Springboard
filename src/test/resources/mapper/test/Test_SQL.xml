<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="test">

	<select id="selectStaticTestFile" resultType="String">
    	<![CDATA[
        	SELECT
				STORED_FILE_NAME
			FROM 
				tb_file
			WHERE
            	idx = 1
    	]]>
    </select>
	
</mapper>